<% if current_user %>
  <%= link_to "Logout", session_path(session[:id]), :method => :delete %>
  <%= link_to "Add Contest", new_contest_path %>
  <%= link_to "Find a Contest", categories_path %>
  <%= link_to "Profile", user_path(current_user) %>
<% else %>
  <%= link_to "Login", new_session_path %>
  <%= link_to "Sign up", new_user_path %>
<% end %>
